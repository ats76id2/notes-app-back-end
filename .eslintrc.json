{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style":"off",
        "no-trailing-spaces":"off",
        "no-unused-vars":"off",
        "padded-blocks":"off",
        "indent":"off",
        "import/no-extraneous-dependencies":"off",
        "no-multiple-empty-lines":"off",
        "consistent-return":"off",
        "max-len":"off",
        "no-underscore-dangle":"off",
        "no-empty-function":"off",
        "constructor-super":"off"
    }
}
